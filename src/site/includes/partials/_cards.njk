  <section id="start-here" class="cards">
  {% for card in cards.cards %}
    <div class="cards__card">
      <h3 class="cards__card--hed">{{ card.hed }}</h3>
      <div class="cards__card--body">
        <p class="cards__card--dek">{{ card.dek }}</p>
        {% if card.links %}
        <footer class="cards__card--foot">
        {% for link in card.links %}
          {% set regExpLink = r/^#.*/g %}
          <a class="cards__card--cta switchoff {{ 'inbound' if regExpLink.test( link.url) }}" href="{{ link.url }}">{{link.cta}}</a>
        {% endfor %}
        </footer>
      </div>
      {% endif %}
    </div>

  {% endfor %}
  </section>
